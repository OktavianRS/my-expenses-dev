script(src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.0-beta.2/angular.min.js")
script(src="/controllers/controller.js")
script(src="/js/mainMenu.js")
script(src="/js/menu/modernizr.custom.js")
